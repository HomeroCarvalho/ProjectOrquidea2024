[*.cs]

# CS8603: Possível retorno de referência nula.
dotnet_diagnostic.CS8603.severity = none

# IDE0003: Remover qualificação
dotnet_diagnostic.IDE0003.severity = none

# Default severity for all analyzer diagnostics
dotnet_analyzer_diagnostic.severity = none

# CS0612: O tipo ou membro é obsoleto
dotnet_diagnostic.CS0612.severity = none

# CS8600: Conversão de literal nula ou possível valor nulo em tipo não anulável.
dotnet_diagnostic.CS8600.severity = none

# CS8601: Possível atribuição de referência nula.
dotnet_diagnostic.CS8601.severity = none

# CS8604: Possível argumento de referência nula.
dotnet_diagnostic.CS8604.severity = none

# CS8625: Não é possível converter um literal nulo em um tipo de referência não anulável.
dotnet_diagnostic.CS8625.severity = none

# CS8602: Desreferência de uma referência possivelmente nula.
dotnet_diagnostic.CS8602.severity = none

# CS8618: O campo não anulável precisa conter um valor não nulo ao sair do construtor. Considere declará-lo como anulável.
dotnet_diagnostic.CS8618.severity = none

# CS8767: A nulidade de tipos de referência no tipo de parâmetro não corresponde ao membro implementado implicitamente (possivelmente devido a atributos de nulidade).
dotnet_diagnostic.CS8767.severity = none

# CS8605: Executando a conversão unboxing de um valor possivelmente nulo.
dotnet_diagnostic.CS8605.severity = none

# CS0472: O resultado da expressão é sempre o mesmo, pois um valor deste tipo nunca é 'null' 
dotnet_diagnostic.CS0472.severity = none

# CS0219: A variável é atribuída, mas seu valor nunca é usado
dotnet_diagnostic.CS0219.severity = none
